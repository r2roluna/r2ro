<!-- Contact Me  -->
<div id="contact-me">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-6 col-md-6 col-sm-10 col-lg-offset-3 col-md-offset-3 col-sm-offset-3">
        <h2>Let's chat!</h2>
        <form id="contact-form" onsubmit"submitForm(); return false;" class="form-horizontal">
        <div id="msg" class="alert"></div>
          <!-- Name  -->
          <div class="form-group">
            <label for="name">Name <span>*</span></label>
            <input type="text" class="form-control" id="name" placeholder="Name">
          </div>
          <!-- Email Address  -->
          <div class="form-group">
            <label for="email">Email Address <span>*</span></label>
            <input type="email" class="form-control" id="email" placeholder="E-mail">
          </div>
          <!-- Subject  -->
          <div class="form-group">
            <label for="subject">Subject: <span>*</span></label>
            <input type="text" class="form-control" id="subject" placeholder="Subject">
          </div>
          <!-- Message -->
          <div class="form-group">
            <label for="message">Message: <span>*</span></label>
            <textarea class="form-control" rows="5" id="message" placeholder="Message"></textarea>
          </div>
          <button type="submit" id="mybtn" class="btn btn-primary btn-lg send">Send</button><span id="status"></span>
          <button type="reset" class="btn btn-danger btn-lg reset">Reset</button>
        </form>
      </div>
    </div>
  </div>
</div>
